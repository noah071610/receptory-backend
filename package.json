{
  "name": "receptori-backend",
  "version": "1.0.0",
  "description": "",
  "author": "Noah",
  "private": true,
  "license": "ISC",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production pm2 start dist/src/main.js",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:migrate": "npx prisma migrate dev --name init && ts-node prisma/seed/seed.ts",
    "db:reset": "npx prisma migrate reset && npx prisma migrate dev --name init && ts-node prisma/seed/seed.ts && npm run dev",
    "db:seed": "ts-node prisma/seed/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.569.0",
    "@aws-sdk/s3-request-presigner": "3.569.0",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "10.3.8",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/throttler": "5.1.2",
    "@prisma/client": "5.15.0",
    "bcrypt": "5.1.1",
    "cache-manager": "5.5.2",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "express-rate-limit": "7.2.0",
    "express-session": "1.18.0",
    "helmet": "7.1.0",
    "hpp": "0.2.3",
    "immer": "10.1.1",
    "ioredis": "5.4.1",
    "nanoid": "3.3.4",
    "passport": "0.7.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "1.4.11",
    "@types/node": "^20.12.10",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "cross-env": "7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "pm2": "5.4.0",
    "prettier": "^3.2.5",
    "prisma": "5.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
